  "devDependencies": {
    "@storybook/addon-essentials": "latest",
    "@storybook/blocks": "latest",
    "@storybook/web-components": "latest",
    "@storybook/web-components-vite": "latest",
    "@whitespace/storybook-addon-html": "latest",
    "gh-pages": "latest",
    "react": "latest",
    "react-dom": "latest",
    "storybook": "latest",
    "typescript": "latest",
    "vite": "latest"
  }
