{
  "name": "scroll-spy-web-component",
  "version": "1.0.0",
  "description": "A component that will highlight navigation links when the corresponding article anchor is in view",
  "source": "index.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/scroll-spy.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "storybook": "storybook dev -p 6006",
